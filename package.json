{
  "name": "angular2-basic",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "aot": "ngc -p ./tsconfig.aot.json",
    "build:pro": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build:aot": "npm run clean:aot && npm run aot && webpack --config config/webpack.aot.js --profile --bail",
    "build": "npm run build:pro",
    "ci": "npm run lint && npm test && npm run e2e",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run protractor",
    "protractor": "protractor config/protractor.conf.js",
    "test": "npm run test:pro",
    "test:pro": "karma start",
    "test:dev": "karma start --dev",
    "tslint": "tslint",
    "docs": "npm run typedoc -- --options config/typedoc.json --exclude '**/*.spec.ts' ./src/",
    "typedoc": "typedoc",
    "clean:aot": "npm run rimraf -- dist",
    "clean:dist": "npm run rimraf -- dist && npm run rimraf -- dist-aot",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "rimraf": "rimraf",
    "serve:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/ --inline --hot",
    "serve:pro": "npm run build:pro & http-server dist --cors -o",
    "serve": "npm run serve:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "@angular/common": "2.1.1",
    "@angular/compiler": "2.1.1",
    "@angular/compiler-cli": "2.1.1",
    "@angular/core": "2.1.1",
    "@angular/forms": "2.1.1",
    "@angular/http": "2.1.1",
    "@angular/material": "^2.0.0-alpha.10",
    "@angular/platform-browser": "2.1.1",
    "@angular/platform-browser-dynamic": "2.1.1",
    "@angular/platform-server": "2.1.1",
    "@angular/router": "3.1.1",
    "@types/lodash": "4.14.39",
    "assets-webpack-plugin": "3.5.0",
    "core-js": "2.4.1",
    "http-server": "0.9.0",
    "ie-shim": "0.1.0",
    "moment": "2.16.0",
    "ng2-translate": "^4.0.1",
    "rxjs": "5.0.0-rc.4",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@ngtools/webpack": "1.1.7",
    "@types/core-js": "0.9.34",
    "@types/hammerjs": "2.0.33",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "4.14.39",
    "@types/node": "6.0.49",
    "@types/protractor": "1.5.20",
    "@types/selenium-webdriver": "2.53.35",
    "@types/source-map": "0.5.0",
    "@types/uglify-js": "2.6.28",
    "@types/webpack": "1.12.36",
    "angular2-router-loader": "0.3.2",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "2.2.4",
    "codelyzer": "~1.0.0-beta.4",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.25.0",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.1",
    "file-loader": "0.9.0",
    "gh-pages": "0.11.0",
    "html-webpack-plugin": "2.24.1",
    "image-webpack-loader": "^3.0.0",
    "imports-loader": "0.6.5",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-spec-reporter": "^2.7.0",
    "json-loader": "0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-phantomjs-shim": "1.4.0",
    "karma-remap-coverage": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-threshold-reporter": "0.1.15",
    "karma-webpack": "1.8.0",
    "node-sass": "3.13.0",
    "offline-plugin": "3.4.2",
    "open-browser-webpack-plugin": "0.0.3",
    "parse5": "2.2.3",
    "protractor": "4.0.11",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "script-ext-html-webpack-plugin": "1.3.2",
    "source-map-loader": "0.1.5",
    "string-replace-loader": "1.0.5",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
    "ts-helpers": "1.1.2",
    "ts-node": "1.5.0",
    "tslint": "~3.15.1",
    "tslint-loader": "^2.1.5",
    "typedoc": "0.5.1",
    "typescript": "2.0.6",
    "url-loader": "0.5.7",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-middleware": "1.8.4",
    "webpack-dev-server": "2.1.0-beta.9",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "0.15.0"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
